{
    "name": "matteo/flarum-ext-prevent-all-caps-titles",
    "description": "Prevent users to write all caps titles",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "Matteo",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Matteo\\PreventAllCapsTitles\\": "src/"
        }
